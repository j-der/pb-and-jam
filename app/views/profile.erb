<h1>Home Page</h1>
<head>
  <title>PB and JAM</title>
  <meta name="viewport" content="width=device-width">
  <link rel="stylesheet" href="css/normalize.css">
  <link rel="stylesheet" href="/stylesheets/application.css">
  <link href='https://fonts.googleapis.com/css?family=Mrs+Saint+Delafield|Droid+Sans' rel='stylesheet' type='text/css'>
</head>
<body>
  <p><img class="pic" src="/uploads/<%= @user.id %>"></p>
    <h1>Hello, <%= @user.username %>!</h1>

  <p>
    Your name is <%= @user.first_name %> <%= @user.last_name %>
  </p>
  <p>
  Instrument of choice: <br/>
  <%= @user.instrument %>
  </p>
  <p>
  Musical style: <br/>
  <%= @user.style %>
  </p>
  <p>
  About: <br/>
  <%= @user.about %>
  </p>
  <form method='get' action='/logout'>
  <div>
    <input type="submit" value="Log out">
  </div>
</body>
